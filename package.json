        "es6-promise": "4.2.8",
        "express": "4.17.1",
        "express-limiter": "1.6.1",
        "helmet": "3.21.2",
        "http-status-codes": "1.4.0",
        "jsonwebtoken": "8.5.1",
        "morgan": "1.9.1",
        "mysql": "2.17.1",
        "rate-limiter-flexible": "1.2.0",
        "redis": "2.8.0",
